    <s:String x:Key="Darkin1" xml:space="preserve">&#10;Posiadacz jest także Demonem.</s:String>
    <!--Force of Nature-->
    <s:String x:Key="ForceOfNature" xml:space="preserve">Siła Natury</s:String>
