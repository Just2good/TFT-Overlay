    <s:String x:Key="ClassOrigins_Wild_Describe3" xml:space="preserve">Tylko Dzicy (2) Wszyscy sojusznicy (4)</s:String>

    <!-- Yordle -->
    <s:String x:Key="ClassOrigins_Yordle" xml:space="preserve">Yordle</s:String>
