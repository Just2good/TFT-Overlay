    <!--Youmuu's Ghostblade-->
    <s:String x:Key="YoumuusGhostblade" xml:space="preserve">Widmowe Ostrze Youmuu</s:String>
    <s:String x:Key="YoumuusGhostblade1" xml:space="preserve">&#10;Posiadacz jest także Zabójcą.</s:String>
    <!--Yuumi-->
    <s:String x:Key="Yuumi" xml:space="preserve">Yuumi</s:String>
    <s:String x:Key="Yuumi1" xml:space="preserve">&#10;Posiadacz jest także Czarodziejem.</s:String>

    <!--Descriptions by Object#3031-->
    <!--Bloodthirster-->
    <s:String x:Key="Bloodthirster" xml:space="preserve">Krwiopijec</s:String>
    <s:String x:Key="Bloodthirster1" xml:space="preserve">&#10;Ataki leczą za 50% obrażeń.</s:String>
    <!--Cursed Blade-->
    <s:String x:Key="CursedBlade" xml:space="preserve">Ostrze Derwisza</s:String>
    <s:String x:Key="CursedBlade1" xml:space="preserve">&#10;Ataki mają 20% szansy na skurczenie (Zredukowanie poziomu gwiazdki o 1 do 0).</s:String>
    <!--Dragon's Claw-->
    <s:String x:Key="DragonsClaw" xml:space="preserve">Smoczy Pazur</s:String>
    <s:String x:Key="DragonClaw1" xml:space="preserve">&#10;Daje 83% odporności na magiczne obrażenia.</s:String>
    <!--Frozen Heart-->
    <s:String x:Key="FrozenHeart" xml:space="preserve">Mroźne Serce</s:String>
    <s:String x:Key="FrozenHeart1" xml:space="preserve">&#10;Sąsiadujący przeciwnicy tracą 25% prędkości ataku.</s:String>
    <!--Guardian Angel-->
    <s:String x:Key="GuardianAngel" xml:space="preserve">Anioł Stróż</s:String>
    <s:String x:Key="GuardianAngel1" xml:space="preserve">&#10;Posiadacz odradza się z 1000 punktów zdrowia po 2 sekundach.</s:String>
    <!--Guinsoo's Rageblade-->
    <s:String x:Key="GuinsoosRageblade" xml:space="preserve">Ostrze Gniewu Guinsoo</s:String>
    <s:String x:Key="GuinsoosRageblade1" xml:space="preserve">&#10;Atak zapewnia dodatkowe 4% prędkości ataku (Kumuluje się w nieskończoność).</s:String>
    <!--Hextech Gunblade-->
    <s:String x:Key="HextechGunblade" xml:space="preserve">Miecz-Pistolet Hextech</s:String>
    <s:String x:Key="HextechGunblade1" xml:space="preserve">&#10;Leczy za 33% wszystkich zadanych obrażeń.</s:String>
    <!--Hush-->
    <s:String x:Key="Hush" xml:space="preserve">Uciszacz</s:String>
    <s:String x:Key="Hush1" xml:space="preserve">&#10;Ataki mają 50% szans na Uciszenie.</s:String>
    <!--Infinity Edge-->
    <s:String x:Key="InfinityEdge" xml:space="preserve">Ostrze Nieskończoności</s:String>
    <s:String x:Key="InfinityEdge1" xml:space="preserve">&#10;Trafienia krytyczne zadają +100% obrażeń.</s:String>
    <!--Ionic Spark-->
    <s:String x:Key="IonicSpark" xml:space="preserve">Jonowa Iskra</s:String>
    <s:String x:Key="IonicSpark1" xml:space="preserve">&#10;Gdy jakikolwiek przeciwnik użyje umiejętności, otrzymuje on 200 nieuchronnych obrażeń.</s:String>
    <!--Locket of the Iron Solari-->
    <s:String x:Key="LocketSolari" xml:space="preserve">Naszyjnik Żelaznych Solari</s:String>
    <s:String x:Key="LocketSolari1" xml:space="preserve">&#10;Posiadacz i sojusznicy na 2 miejsca od lewej i prawej otrzymują 200 punktów tarczy na początek rundy.</s:String>
    <!--Luden's Echo-->
    <s:String x:Key="LudensEcho" xml:space="preserve">Echo Luden</s:String>
    <s:String x:Key="LudensEcho1" xml:space="preserve">&#10;Umiejętności zadają podstawowo 200 oraz 200 rozpryskowych obrażeń na trafienie.</s:String>
    <!--Morellonomicon-->
    <s:String x:Key="Morello" xml:space="preserve">Morellonomicon</s:String>
    <s:String x:Key="Morello1" xml:space="preserve">&#10;Umiejętnosći palą wrogów za 5% ich maksymalnego zdrowia na sekundę i uniemożliwiają leczenie.</s:String>
    <!--Phantom Dancer-->
    <s:String x:Key="PhantomDancer" xml:space="preserve">Widmowy Pancerz</s:String>
    <s:String x:Key="PhantomDancer1" xml:space="preserve">&#10;Posiadacz unika wszystki Trafienia Krytyczne.</s:String>
    <!--Rabadon's Deathcap-->
    <s:String x:Key="RabadonsDeathcap" xml:space="preserve">Zabójczy Kapelusz Rabadona</s:String>
    <s:String x:Key="RabadonsDeathcap1" xml:space="preserve">&#10;+50% Mocy Umiejętności.</s:String>
    <!--Rapid Firecannon-->
    <s:String x:Key="RapidFirecannon" xml:space="preserve">Ognista Armata</s:String>
    <s:String x:Key="RapidFirecannon1" xml:space="preserve">&#10;Ataki posiadacza nie mogą zostać uniknięte. Zasięg ataku jest podwojony.</s:String>
    <!--Red Buff-->
    <s:String x:Key="RedBuff" xml:space="preserve">Czerwone Wzmocnienie</s:String>
    <s:String x:Key="RedBuff1" xml:space="preserve">&#10;Ataki palą wrogów za 13% ich maksymalnego zdrowia i uniemożliwiają leczenie.</s:String>
    <!--Redemption-->
    <s:String x:Key="Redemption" xml:space="preserve">Odkupienie</s:String>
    <s:String x:Key="Redemption1" xml:space="preserve">&#10;Kiedy posiadacz ma 25% zdrowia leczy pobliskich sojuszników o 1000 punktów zdrowia.</s:String>
    <!--Seraph's Embrace-->
    <s:String x:Key="SeraphsEmbrace" xml:space="preserve">Uścisk Serafina</s:String>
    <s:String x:Key="SeraphsEmbrace1" xml:space="preserve">&#10;Odnawia 20 many za każym razem kiedy używana jest umiejętność.</s:String>
    <!--Spear of Shojin-->
    <s:String x:Key="SpearofShojin" xml:space="preserve">Włócznia Shojina</s:String>
    <s:String x:Key="SpearofShojin1" xml:space="preserve">&#10;Po użyciu umiejętności, posiadacz odnawia 15% many po każdym ataku.</s:String>
    <!--Spear of Shojin-->
    <s:String x:Key="StatikkShiv" xml:space="preserve">Kosa Statikka</s:String>
    <s:String x:Key="StatikkShiv1" xml:space="preserve">&#10;Co każdy 3 atak zadaje 100 magicznych obrażeń 3 dodatkowym wrogom.</s:String>
    <!--Sword Breaker-->
    <s:String x:Key="SwordBreaker" xml:space="preserve">Łamimiecz</s:String>
    <s:String x:Key="SwordBreaker1" xml:space="preserve">&#10;Ataki mają 25% szanse na rozbrojenie.</s:String>
    <!--Sword of the Divine-->
    <s:String x:Key="SwordDivine" xml:space="preserve">Boski Miecz</s:String>
    <s:String x:Key="SwordDivine1" xml:space="preserve">&#10;Co każdą sekundę, ma 5% szans na zdobycie 100% szans na Trafienie Krytyczne.</s:String>
    <!--Thornmail-->
    <s:String x:Key="Thornmail" xml:space="preserve">Kolczasta Kolczuga</s:String>
    <s:String x:Key="Thornmail1" xml:space="preserve">&#10;Odbija 100% otrzymanych fizycznych obrażeń.</s:String>
    <!--Titanic Hydra-->
    <s:String x:Key="TitanicHydra" xml:space="preserve">Kolosalna Hydra</s:String>
    <s:String x:Key="TitanicHydra1" xml:space="preserve">&#10;Ataki zadają 10% maksymalnego zdrowia posiadacza jako obrażenia rozpryskowe.</s:String>
    <!--Warmog's Armor-->
    <s:String x:Key="WarmogsArmor" xml:space="preserve">Plemienna Zbroja</s:String>
    <s:String x:Key="WarmogsArmor1" xml:space="preserve">&#10;Posiadacz odnawia 6% brakującego zdrowia na sekundę.</s:String>
    <!--Zeke's Herald-->
    <s:String x:Key="ZekesHerald" xml:space="preserve">Herold Zeke'a</s:String>
    <s:String x:Key="ZekesHerald1" xml:space="preserve">&#10;Posiadacz i sojusznicy na 2 miejsca od lewej i prawej otrzymują 15% prędkości ataku na początek rundy.</s:String>
    <!--Zephyr-->
    <s:String x:Key="Zephyr" xml:space="preserve">Zefir</s:String>
    <s:String x:Key="Zephyr1" xml:space="preserve">&#10;Na początku rundy, wyklucza jednego przeciwnika na 5 sekund.</s:String>

