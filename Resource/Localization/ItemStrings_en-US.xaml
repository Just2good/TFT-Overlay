    <s:String x:Key="Origins_Noble_Describe2" xml:space="preserve">(3) One Random Ally (6) All Allies</s:String>

    <!-- Ocean -->
    <s:String x:Key="Origins_Ocean" xml:space="preserve">Ocean</s:String>
    <s:String x:Key="Origins_Ocean_Describe1" xml:space="preserve">All allies restore mana every 4 seconds</s:String>
    <s:String x:Key="Origins_Ocean_Describe2" xml:space="preserve">(2) +10 mana (4) +30 mana (6) +60 mana</s:String>

    <!-- Phantom -->
    <s:String x:Key="Origins_Phantom" xml:space="preserve">Phantom</s:String>
    <s:String x:Key="Origins_Phantom_Describe1" xml:space="preserve">(2) Curse a random enemy at the start of combat, setting their HP to 100</s:String>

    <!-- Pirate -->
