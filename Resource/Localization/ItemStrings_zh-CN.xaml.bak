    <s:String x:Key="Charts_Damage" xml:space="preserve">玩家伤害公式</s:String>
    <s:String x:Key="Charts_Damage1" xml:space="preserve">2点(小小英雄) + </s:String>
    <s:String x:Key="Charts_Damage2" xml:space="preserve">每个单位的伤害</s:String>

