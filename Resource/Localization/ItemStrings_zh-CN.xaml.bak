    <s:String x:Key="Origins_Pirate_Describe1" xml:space="preserve">赚取最多4枚金币在与另一名玩家战斗之后 (3)</s:String>

    <!-- Poison -->
    <s:String x:Key="Origins_Poison" xml:space="preserve">剧毒</s:String>
    <s:String x:Key="Origins_Poison_Describe1" xml:space="preserve">(3) 造成伤害时，增加被攻击目标 50% 的蓝耗</s:String>

    <!-- Robot -->
    <s:String x:Key="Origins_Robot" xml:space="preserve">机器人</s:String>
    <s:String x:Key="Origins_Robot_Describe1" xml:space="preserve">机器人在开始战斗时就有满额的法力值 (1)</s:String>

    <!-- Shadow -->
    <s:String x:Key="Origins_Shadow" xml:space="preserve">风影</s:String>
    <s:String x:Key="Origins_Shadow_Describe1" xml:space="preserve">当风影英雄造成伤害时，在 5 秒内提升</s:String>
    <s:String x:Key="Origins_Shadow_Describe2" xml:space="preserve">(2) 自身 50% 的伤害 (4) 全体风影英雄 100% 的伤害</s:String>

    <!-- Steel -->
    <s:String x:Key="Origins_Steel" xml:space="preserve">合金</s:String>
    <s:String x:Key="Origins_Steel_Describe1" xml:space="preserve">合金英雄生命值低于 50% 时，获得免疫伤害，持续</s:String>
    <s:String x:Key="Origins_Steel_Describe2" xml:space="preserve">(2) 2 秒 (3) 3 秒 (4) 4 秒</s:String>

    <!-- Variable -->
    <s:String x:Key="Origins_Variable" xml:space="preserve">大元素使</s:String>
    <s:String x:Key="Origins_Variable_Describe1" xml:space="preserve">这些元素使者可以拥有任何元素特质</s:String>

    <!-- Void -->
    <s:String x:Key="Origins_Void" xml:space="preserve">虚空</s:String>
    <s:String x:Key="Origins_Void_Describe1" xml:space="preserve">(2) 一虚空单位造成真实伤害 (4) 所有虚空单位造成真实伤害</s:String>

    <!-- Wild -->
    <s:String x:Key="Origins_Wild" xml:space="preserve">狂野</s:String>
    <s:String x:Key="Origins_Wild_Describe1" xml:space="preserve">攻击会生成怒气层数(最多可叠5层),每层怒气提供10%攻击速度,最高60%</s:String>
    <s:String x:Key="Origins_Wild_Describe2" xml:space="preserve">(2) 狂野单位 (4) 所有友军且攻击不能被闪避</s:String>

    <!-- Woodland -->
    <s:String x:Key="Origins_Woodland" xml:space="preserve">林地</s:String>
    <s:String x:Key="Origins_Woodland_Describe1" xml:space="preserve">(3) 战斗开始时，随机复制一个林地英雄的镜像</s:String>

    <!-- Yordle -->
    <s:String x:Key="Origins_Yordle" xml:space="preserve">约德尔人</s:String>
    <s:String x:Key="Origins_Yordle_Describe1" xml:space="preserve">友方约德尔人有一定几率闪避攻击和攻击特效</s:String>
    <s:String x:Key="Origins_Yordle_Describe2" xml:space="preserve">30%几率闪避(3)  60%几率闪避(6)  90%几率闪避(9)</s:String>

    <!--Classes | Effects and Champions-->
    <s:String x:Key="Class_Label" xml:space="preserve">职业 | 羁绊效果</s:String>

    <!-- Alchemist -->
    <s:String x:Key="Class_Alchemist" xml:space="preserve">炼金师</s:String>
    <s:String x:Key="Class_Alchemist_Describe1" xml:space="preserve">炼金师会穿过其他单位并且永不停止移动</s:String>

    <!-- Assassin -->
    <s:String x:Key="Class_Assassin" xml:space="preserve">刺客</s:String>
    <s:String x:Key="Class_Assassin_Describe1" xml:space="preserve">刺客们会获得额外的暴击伤害和暴击几率</s:String>
    <s:String x:Key="Class_Assassin_Describe2" xml:space="preserve">(3) 10% 的几率造成 75% 的暴伤 (6) 20% 的几率造成 150% 的暴伤</s:String>

    <!-- Avatar -->
    <s:String x:Key="Class_Avatar" xml:space="preserve">大元素使</s:String>
    <s:String x:Key="Class_Avatar_Describe1" xml:space="preserve">大元素使在计算元素特质加成时按双倍计算</s:String>

    <!-- Berserker -->
    <s:String x:Key="Class_Berserker" xml:space="preserve">狂战士</s:String>
    <s:String x:Key="Class_Berserker_Describe1" xml:space="preserve">狂战士的攻击有几率对目标背后锥形区域造成伤害</s:String>
    <s:String x:Key="Class_Berserker_Describe2" xml:space="preserve">(3) 40% 几率 (6) 100% 几率</s:String>

    <!-- Blademaster -->
    <s:String x:Key="Class_Blademaster" xml:space="preserve">剑士</s:String>
    <s:String x:Key="Class_Blademaster_Describe1" xml:space="preserve">剑士们有 40% 几率打出一定次数的额外攻击(每次普攻)</s:String>
    <s:String x:Key="Class_Blademaster_Describe2" xml:space="preserve">(2) 1次额外攻击 (4) 2次额外攻击 (6) 3次额外攻击</s:String>

    <!-- Brawler -->
    <s:String x:Key="Class_Brawler" xml:space="preserve">斗士</s:String>
    <s:String x:Key="Class_Brawler_Describe1" xml:space="preserve">斗士获得额外的最大生命值</s:String>
    <s:String x:Key="Class_Brawler_Describe2" xml:space="preserve">250额外生命值(2) 500额外生命值(4) 900额外生命值</s:String>

    <!-- Druid -->
    <s:String x:Key="Class_Druid" xml:space="preserve">德鲁伊</s:String>
    <s:String x:Key="Class_Druid_Describe1" xml:space="preserve">(2) 德鲁伊获得每秒 40 点的生命回复</s:String>

    <!-- Elementalist -->
    <s:String x:Key="Class_Elementalist" xml:space="preserve">元素使</s:String>
    <s:String x:Key="Class_Elementalist_Describe1" xml:space="preserve">元素使们的攻击提供双倍法力值(普攻时)在战斗开始时, 召唤小菊(3)</s:String>

    <!-- Guardian -->
    <s:String x:Key="Class_Guardian" xml:space="preserve">护卫</s:String>
    <s:String x:Key="Class_Guardian_Describe1" xml:space="preserve">在战斗开始时，所有护卫邻近格子的友军 (2)+40护甲护卫们不会给自己提供增益</s:String>

    <!-- Gunslinger -->
    <s:String x:Key="Class_Gunslinger" xml:space="preserve">枪手</s:String>
    <s:String x:Key="Class_Gunslinger_Describe1" xml:space="preserve">枪手们的攻击有50%几率打出额外的攻击攻击另一名随机</s:String>
    <s:String x:Key="Class_Gunslinger_Describe2" xml:space="preserve">敌人(2) 攻击额外2名随机敌人(4) 攻击4名随机敌人(6)(在攻击范围内)</s:String>

    <!-- Knight -->
    <s:String x:Key="Class_Knight" xml:space="preserve">骑士</s:String>
    <s:String x:Key="Class_Knight_Describe1" xml:space="preserve">你的队伍无视来自所有来源的</s:String>
    <s:String x:Key="Class_Knight_Describe2" xml:space="preserve">(2) 15点伤害(4) 35点伤害(6) 65点伤害</s:String>

    <!-- Mage -->
    <s:String x:Key="Class_Mage" xml:space="preserve">魔法师</s:String>
    <s:String x:Key="Class_Mage_Describe1" xml:space="preserve">魔法师有几率在释放完技能后再次释放技能</s:String>
    <s:String x:Key="Class_Mage_Describe2" xml:space="preserve">(3) 50% 几率 (6) 100% 几率</s:String>

    <!-- Mystic -->
    <s:String x:Key="Class_Mystic" xml:space="preserve">秘术师</s:String>
    <s:String x:Key="Class_Mystic_Describe1" xml:space="preserve">为全体友军增加魔法抗性</s:String>
    <s:String x:Key="Class_Mystic_Describe2" xml:space="preserve">(2) 40 魔抗 (4) 120 魔抗</s:String>

    <!-- Predator -->
    <s:String x:Key="Class_Predator" xml:space="preserve">掠食者</s:String>
    <s:String x:Key="Class_Predator_Describe1" xml:space="preserve">(3) 掠食者们的伤害会秒杀生命值低于 20% 的敌人</s:String>

    <!-- Ranger -->
    <s:String x:Key="Class_Ranger" xml:space="preserve">游侠</s:String>
    <s:String x:Key="Class_Ranger_Describe1" xml:space="preserve">游侠们每 3 秒都有一定几率来使他们在接下来的 3 秒内攻速翻倍</s:String>
    <s:String x:Key="Class_Ranger_Describe2" xml:space="preserve">(2) 30% 几率使攻速翻倍  (4) 60% 几率使攻速翻倍 (6) 100% 几率使攻速翻倍</s:String>

    <!-- Shapeshifter -->
    <s:String x:Key="Class_Shapeshifter" xml:space="preserve">换形师</s:String>
    <s:String x:Key="Class_Shapeshifter_Describe1" xml:space="preserve">换形师们会在他们变形时获得额外的最大生命值。</s:String>
    <s:String x:Key="Class_Shapeshifter_Describe2" xml:space="preserve">(3) +60%额外最大生命值 (6) +100%额外最大生命值</s:String>

    <!-- Sorcerer -->
    <s:String x:Key="Class_Sorcerer" xml:space="preserve">法师</s:String>
    <s:String x:Key="Class_Sorcerer_Describe1" xml:space="preserve">法师们的攻击提供双倍魔法值。友军的技能伤害得到提升</s:String>
    <s:String x:Key="Class_Sorcerer_Describe2" xml:space="preserve">+40%技能伤害(3) +120%技能伤害(6) +200%技能伤害(9)</s:String>

    <!-- Summoner -->
    <s:String x:Key="Class_Summoner" xml:space="preserve">召唤师</s:String>
    <s:String x:Key="Class_Summoner_Describe1" xml:space="preserve">增加召唤精灵的生命值和持续时间</s:String>
    <s:String x:Key="Class_Summoner_Describe2" xml:space="preserve">(3) 40% 提升 (6) 100% 提升</s:String>

    <!-- Warden -->
    <s:String x:Key="Class_Warden" xml:space="preserve">守护神</s:String>
    <s:String x:Key="Class_Warden_Describe1" xml:space="preserve">守护者提升所有友军的护甲</s:String>
    <s:String x:Key="Class_Warden_Describe2" xml:space="preserve">(2) +125% 护甲 (4) +275% 护甲 (6) +450% 护甲</s:String>

