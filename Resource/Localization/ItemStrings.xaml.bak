    <!--Elements | Effects and Champions-->
    <s:String x:Key="Origins_Label" xml:space="preserve">Elements | Effects and Champions</s:String>

    <!-- Cloud -->
    <s:String x:Key="Origins_Cloud" xml:space="preserve">Cloud</s:String>
    <s:String x:Key="Origins_Cloud_Describe1" xml:space="preserve">All allies gain dodge chance</s:String>
    <s:String x:Key="Origins_Cloud_Describe2" xml:space="preserve">(2) 20% dodge chance (3) 25% dodge chance (4) 35% dodge chance</s:String>

    <!-- Crystal -->
    <s:String x:Key="Origins_Crystal" xml:space="preserve">Crystal</s:String>
    <s:String x:Key="Origins_Crystal_Describe1" xml:space="preserve">Crystal units have a max amount of damage they can take from a single hit</s:String>
    <s:String x:Key="Origins_Crystal_Describe2" xml:space="preserve">(2) 100 Max Damage (4) 60 Max Damage</s:String>

    <!-- Demons -->
    <s:String x:Key="Origins_Demons" xml:space="preserve">Demons</s:String>
    <s:String x:Key="Origins_Demons_Describe1" xml:space="preserve">40% chance on-hit to burn 20 mana from their target and gain</s:String>
    <s:String x:Key="Origins_Demons_Describe2" xml:space="preserve">(2) 15, (4) 30, (6) 45 mana</s:String>

    <!-- Desert -->
    <s:String x:Key="Origins_Desert" xml:space="preserve">Desert</s:String>
    <s:String x:Key="Origins_Desert_Describe1" xml:space="preserve">Reduces each enemy's armor</s:String>
    <s:String x:Key="Origins_Desert_Describe2" xml:space="preserve">(2) 50% Armor reduction (4) 90% Armor reduction</s:String>

    <!-- Dragon -->
    <s:String x:Key="Origins_Dragon" xml:space="preserve">Dragon</s:String>
    <s:String x:Key="Origins_Dragon_Describe1" xml:space="preserve">Dragons take reduced magic damage</s:String>
    <s:String x:Key="Origins_Dragon_Describe2" xml:space="preserve">(2) 50% Magic Immunity</s:String>

    <!-- Electric -->
    <s:String x:Key="Origins_Electric" xml:space="preserve">Electric</s:String>
    <s:String x:Key="Origins_Electric_Describe1" xml:space="preserve">Electric units shock nearby enemies when they deal or receive a critical strike</s:String>
    <s:String x:Key="Origins_Electric_Describe2" xml:space="preserve">(2) 100 Damage (3) 300 Damage (4) 500 Damage</s:String>

    <!-- Exile -->
    <s:String x:Key="Origins_Exile" xml:space="preserve">Exile</s:String>
    <s:String x:Key="Origins_Exile_Describe1" xml:space="preserve">(1) Exiles start combat with a shield equal to 100% max HP when placed adjacement to NO other allied units</s:String>

    <!-- Glacial -->
    <s:String x:Key="Origins_Glacial" xml:space="preserve">Glacial</s:String>
    <s:String x:Key="Origins_Glacial_Describe1" xml:space="preserve">On hit, Glacials have a (2) 20%, (4) 35%, (6) 50% to stun for 1.5 seconds</s:String>

    <!-- Hextech -->
    <s:String x:Key="Origins_Hextech" xml:space="preserve">Hextech</s:String>
    <s:String x:Key="Origins_Hextech_Describe1" xml:space="preserve">At the start of combat, throws a bomb at an enemy unit with an item, and disables all nearby items for 5 seconds</s:String>
    <s:String x:Key="Origins_Hextech_Describe2" xml:space="preserve">(2) Within a 1 hex radius (4) Within a 2 hex radius</s:String>

    <!-- Imperial -->
    <s:String x:Key="Origins_Imperial" xml:space="preserve">Imperial</s:String>
    <s:String x:Key="Origins_Imperial_Describe1" xml:space="preserve">Imperials deal Double Damage</s:String>
    <s:String x:Key="Origins_Imperial_Describe2" xml:space="preserve">(1) 1 Random Imperial (4) All Imperials</s:String>

    <!-- Inferno -->
    <s:String x:Key="Origins_Inferno" xml:space="preserve">Inferno</s:String>
    <s:String x:Key="Origins_Inferno_Describe1" xml:space="preserve">Infernos spell damage and crits burn the ground beneath their target, dealing</s:String>
    <s:String x:Key="Origins_Inferno_Describe2" xml:space="preserve">(3) +70% (6) +120% 3 hexes (9) +180% 5 hexes bonus damage over 4 seconds</s:String>

    <!-- Light -->
    <s:String x:Key="Origins_Light" xml:space="preserve">Light</s:String>
    <s:String x:Key="Origins_Light_Describe1" xml:space="preserve">When a Light unit dies, all other Light units gain Attack Speed and are healed for 25% of the dying Champ's Max HP</s:String>
    <s:String x:Key="Origins_Light_Describe2" xml:space="preserve">(3) +15% AS (6) +35% AS (9) +55% AS</s:String>

    <!-- Mountain -->
    <s:String x:Key="Origins_Mountain" xml:space="preserve">Mountain</s:String>
    <s:String x:Key="Origins_Mountain_Describe1" xml:space="preserve">(2) At the start of combat, one random ally gains a 1500 health Stoneshield</s:String>

    <!-- Ninja -->
    <s:String x:Key="Origins_Ninja" xml:space="preserve">Ninja</s:String>
    <s:String x:Key="Origins_Ninja_Describe1" xml:space="preserve">Ninjas gain a flat increase of Attack Damage + Ability Power</s:String>
    <s:String x:Key="Origins_Ninja_Describe3" xml:space="preserve">(Exactly 1) Ninja gains +50 AD + AP (4) All Ninjas gain +80 AD + AP</s:String>

    <!-- Noble -->
    <s:String x:Key="Origins_Noble" xml:space="preserve">Noble</s:String>
    <s:String x:Key="Origins_Noble_Describe1" xml:space="preserve">Nobles grant 50 Armor + MR and heal for 30 on hit</s:String>
    <s:String x:Key="Origins_Noble_Describe2" xml:space="preserve">(3) One Random Ally (6) All Allies</s:String>

    <!-- Ocean -->
    <s:String x:Key="Origins_Ocean" xml:space="preserve">Ocean</s:String>
    <s:String x:Key="Origins_Ocean_Describe1" xml:space="preserve">All allies restore mana every 4 seconds</s:String>
    <s:String x:Key="Origins_Ocean_Describe2" xml:space="preserve">(2) +10 mana (4) +30 mana (6) +60 mana</s:String>

    <!-- Phantom -->
    <s:String x:Key="Origins_Phantom" xml:space="preserve">Phantom</s:String>
    <s:String x:Key="Origins_Phantom_Describe1" xml:space="preserve">(2) Curse a random enemy at the start of combat, setting their HP to 100</s:String>

    <!-- Pirate -->
    <s:String x:Key="Origins_Pirate" xml:space="preserve">Pirate</s:String>
    <s:String x:Key="Origins_Pirate_Describe1" xml:space="preserve">(3) Earn up to 4 additional gold after combat against another player (1.75 avg per chest)</s:String>

    <!-- Poison -->
    <s:String x:Key="Origins_Poison" xml:space="preserve">Poison</s:String>
    <s:String x:Key="Origins_Poison_Describe1" xml:space="preserve">(3) Poison units apply Neurotoxin when they deal damage, increasing the target's mana cost by 50%</s:String>

    <!-- Robot -->
    <s:String x:Key="Origins_Robot" xml:space="preserve">Robot</s:String>
    <s:String x:Key="Origins_Robot_Describe1" xml:space="preserve">(1) Robots start combat at full mana</s:String>

    <!-- Shadow -->
    <s:String x:Key="Origins_Shadow" xml:space="preserve">Shadow</s:String>
    <s:String x:Key="Origins_Shadow_Describe1" xml:space="preserve">Shadow units deal increased damage for 4 seconds at combat start, refreshed on Takedown</s:String>
    <s:String x:Key="Origins_Shadow_Describe2" xml:space="preserve">(2) 65% Increased Damage, Self Takedown (4) 150% Increased Damage, any Shadow Takedown</s:String>

    <!-- Steel -->
    <s:String x:Key="Origins_Steel" xml:space="preserve">Steel</s:String>
    <s:String x:Key="Origins_Steel_Describe1" xml:space="preserve">Steel units gain damage immunity when they are reduced below 50% health</s:String>
    <s:String x:Key="Origins_Steel_Describe2" xml:space="preserve">(2) 2 seconds of immunity (3) 3 seconds (4) 4 seconds</s:String>

    <!-- Variable -->
    <s:String x:Key="Origins_Variable" xml:space="preserve">Variable</s:String>
    <s:String x:Key="Origins_Variable_Describe1" xml:space="preserve">These champions can have any of the Elemental Traits</s:String>

    <!-- Void -->
    <s:String x:Key="Origins_Void" xml:space="preserve">Void</s:String>
    <s:String x:Key="Origins_Void_Describe1" xml:space="preserve">(2) One random void unit deals True Damage (4) All void units deal True Damage</s:String>

    <!-- Wild -->
    <s:String x:Key="Origins_Wild" xml:space="preserve">Wild</s:String>
    <s:String x:Key="Origins_Wild_Describe1" xml:space="preserve">Attacks generate stacks of Fury (up to 5). Each stack of Fury grants 10% atack speed, up to 60%</s:String>
    <s:String x:Key="Origins_Wild_Describe2" xml:space="preserve">(2) Wild Allies (4) All Allies AND their basic attacks can't miss</s:String>

    <!-- Woodland -->
    <s:String x:Key="Origins_Woodland" xml:space="preserve">Woodland</s:String>
    <s:String x:Key="Origins_Woodland_Describe1" xml:space="preserve">(3) At the start of combat, a random Woodland Champ makes a copy of themselves</s:String>

    <!-- Yordle -->
    <s:String x:Key="Origins_Yordle" xml:space="preserve">Yordle</s:String>
    <s:String x:Key="Origins_Yordle_Describe1" xml:space="preserve">Attacks + On-hit effects against ally Yordles can miss</s:String>
    <s:String x:Key="Origins_Yordle_Describe2" xml:space="preserve">(3) 30% chance (6) 60% chance (9) 90% chance </s:String>

