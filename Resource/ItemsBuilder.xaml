                    <Button Grid.Row="6" Grid.Column="4" Width="20" Command="{Binding ElementName=RecurveBowCounter, Path=UpCommand}" Content="+" Style="{StaticResource ButtonStyle}" />

                    <!-- Tear-->
                    <Image Grid.Row="8" Grid.Column="0" Source="../Images/cstyle/items/Tear.png" Style="{StaticResource ImageStyle}"/>
                    <Button Grid.Row="8" Grid.Column="2" Width="20" Command="{Binding ElementName=TearCounter, Path=DownCommand}" Content="-" Style="{StaticResource ButtonStyle}"/>
