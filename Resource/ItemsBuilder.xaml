                    xmlns:p="clr-namespace:TFT_Overlay.Properties"
                    xmlns:utilities="clr-namespace:TFT_Overlay.Utilities">

    <DataTemplate x:Key="ItemsBuilder"  DataType="{x:Type vm:MainViewModel}">
